			VALUES("ctconstr1","#{ctconstr1,jdbcType=NCHAR}");
			VALUES("ctfstr5","#{ctfstr5,jdbcType=NCHAR}");
			VALUES("ctfstr6","#{ctfstr6,jdbcType=NCHAR}");
			VALUES("ctfstr61","#{ctfstr61,jdbcType=NCHAR}");
