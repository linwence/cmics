        "MCU as MCU, WMSMCU as WMSMCU, RKTYPE, CO, WMSCO, DOCO, DCTO, AN8, CASE WHEN YDDJ > 0 THEN TO_CHAR(TO_DATE(YDDJ+1900000, 'YYYYDDD'), 'YYYY-MM-DD') ELSE '' END AS YDDJ, ALPH, VR01, CASE WHEN TRDJ > 0 THEN TO_CHAR(TO_DATE(TRDJ+1900000, 'YYYYDDD'), 'YYYY-MM-DD') ELSE '' END AS TRDJ, REMARK, AT1, DEPTNAME",
        "from ${tableSchema}.VE8RK001"
    })
