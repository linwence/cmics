log4j.appender.other = org.apache.log4j.DailyRollingFileAppender  
log4j.appender.other.File = ${webapp.root}WEB-INF/logs/other.log  
log4j.appender.other.Append = true  
log4j.appender.other.Threshold = INFO   
log4j.appender.other.layout = org.apache.log4j.PatternLayout  
log4j.appender.other.layout.ConversionPattern = %-d{yyyy-MM-dd HH\:mm\:ss}  [ %t\:%r ] - [ %p ]  %m%n 

log4j.appender.mdm = org.apache.log4j.DailyRollingFileAppender
log4j.appender.mdm.File = ${webapp.root}WEB-INF/logs/mdmlog.log
log4j.appender.mdm.Append = true
log4j.appender.mdm.Threshold = INFO
log4j.appender.mdm.layout = org.apache.log4j.PatternLayout
log4j.appender.mdm.layout.ConversionPattern = %-d{yyyy-MM-dd HH\:mm\:ss}  [ %t\:%r ] - [ %p ]  %m%n
